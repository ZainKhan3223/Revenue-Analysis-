{
  "functions": {
    "backend/index.py": {
      "runtime": "vercel-python@0.1.1"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "backend/index.py"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}